class InStore extends Order {
    String cashierName;
    
    
    public InStore(String restruant, String customerName, int totalItems, double totalAmount, String cashierName) {
        super(restruant, customerName, totalItems, totalAmount);
        this.cashierName = cashierName;
    }

    public void printOrder() {
        super.printOrder();
        System.out.println("Cashier Name: " + cashierName);
    }

	public String getCashierName() {
		return cashierName;
	}
    
}
